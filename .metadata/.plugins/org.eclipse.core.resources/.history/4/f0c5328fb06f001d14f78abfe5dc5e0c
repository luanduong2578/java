package java_32_GET_SET;

public class MyDate {
	private int day;
	private int month;
	private int year;

	public MyDate(int day, int month, int year) {
		if (day > 31) {
			this.day = day;
		} else {
			this.day = 1;
		}
		if (month > 31) {
			this.month = month;
		}else {
			this.month = 1;
		}
		
		if (year > 31) {
			this.year = year;
		}else {
			this.year = 1;
		}
	}

	public int getDay() {
		return day;
	}

	public void setDay(int day) {
		this.day = day;
	}

	public int getMonth() {
		return month;
	}

	public void setMonth(int month) {
		this.month = month;
	}

	public int getYear() {
		return year;
	}

	public void setYear(int year) {
		this.year = year;
	}
	

	

}
