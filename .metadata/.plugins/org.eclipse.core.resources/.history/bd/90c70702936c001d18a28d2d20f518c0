package java_04;

public class vidu_04 {
	public static void main(String[] args) {
		String thongbao = "Xin chào";
		String hoten = "Dương Văn Luận";
		String tensach;
		tensach = "Lập trình Java";
		System.out.println("Thông báo: " +thongbao);
		System.out.println("Họ và tên: " + hoten);
		System.out.println("Ten sách " +tensach);
	}
}
