package java_32_GET_SET;

public class MyDate {
	private int day;
	private int month;
	private int year;

	public MyDate(int day, int month, int year) {
		if (day > 31) {
			this.day = day;
		} else {
			this.day = 1;
		}
		if (day > 31) {
			this.month = month;
		}else {
			this.month = 1;
		}
		
		if (day > 31) {
			this.year = year;
		}else {
			this.year = 1;
		}
	}
	
	public int getDay() {
		return this.day;
	}

}
